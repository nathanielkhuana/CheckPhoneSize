<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/phoneTypeInfo_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:text="Android OS"
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line1"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:background="#c0c0c0"
        app:layout_constraintTop_toBottomOf="@+id/phoneTypeInfo_textView"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/fileFreeSpaceTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Code (File.freeSpace)"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/phoneTypeInfo_textView"
        app:layout_constraintTop_toBottomOf="@+id/line1" />

    <TextView
        android:id="@+id/fileFreeSpaceInMB_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Free Space FileFreeSpace"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/fileFreeSpaceTitle"
        app:layout_constraintTop_toBottomOf="@+id/fileFreeSpaceTitle" />

    <View
        android:id="@+id/line2"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:background="#c0c0c0"
        app:layout_constraintTop_toBottomOf="@+id/fileFreeSpaceInMB_textView"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/storageStatsRepoTitle_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Code (StorageStatsRepository)"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/fileFreeSpaceInMB_textView"
        app:layout_constraintTop_toBottomOf="@+id/line2" />

    <TextView
        android:id="@+id/storageStatsRepositoryFreeSpaceInMB_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Free Space StorageStatsRepository"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/storageStatsRepoTitle_textView"
        app:layout_constraintTop_toBottomOf="@+id/storageStatsRepoTitle_textView" />

    <View
        android:id="@+id/line3"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:background="#c0c0c0"
        app:layout_constraintTop_toBottomOf="@+id/storageStatsRepositoryFreeSpaceInMB_textView"
        tools:layout_editor_absoluteX="-16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Difference of Free Space"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/storageStatsRepositoryFreeSpaceInMB_textView"
        app:layout_constraintTop_toBottomOf="@+id/line3" />

    <TextView
        android:id="@+id/freeSpaceDiff_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Free Space Difference"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/freeSpaceDiffInPercentage_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Free Space Difference in %"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/freeSpaceDiff_textView"
        app:layout_constraintTop_toBottomOf="@+id/freeSpaceDiff_textView" />

    <View
        android:id="@+id/line4"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="10dp"
        android:background="#c0c0c0"
        app:layout_constraintTop_toBottomOf="@+id/freeSpaceDiffInPercentage_textView"
        tools:layout_editor_absoluteX="-36dp" />

</androidx.constraintlayout.widget.ConstraintLayout>